<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Propile">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/next2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:onClick="moveToDiscover"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.022"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2"
        app:layout_constraintVertical_bias="0.983"
        app:srcCompat="?attr/actionModeCloseDrawable" />

    <TextView
        android:id="@+id/profileNamee"
        android:layout_width="438dp"
        android:layout_height="167dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@color/colorPrimaryDark"
        android:gravity="center"
        android:text="profileName"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.813"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ScrollView
        android:id="@+id/scroll"
        android:layout_width="match_parent"
        android:layout_height="480dp"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileNamee"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="UnknownId">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/freg23"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="85dp"
                android:background="@color/colorPrimaryDark"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <RelativeLayout
                    android:id="@+id/alayout013"
                    android:layout_width="510dp"
                    android:layout_height="191dp"
                    android:layout_alignTop="@+id/freg2"
                    android:layout_margin="1dp"
                    android:layout_weight=".50"
                    android:background="@color/colorAccent"
                    android:gravity="center"
                    tools:ignore="Suspicious0dp">

                    <TextView
                        android:id="@+id/a1"
                        android:layout_width="202dp"
                        android:layout_height="25dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="139dp"
                        android:layout_marginLeft="139dp"
                        android:layout_marginTop="77dp"
                        android:layout_marginEnd="169dp"
                        android:layout_marginRight="169dp"
                        android:text="Accept push notification\n"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/a11"
                        android:layout_width="258dp"
                        android:layout_height="57dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="116dp"
                        android:layout_marginLeft="116dp"
                        android:layout_marginTop="111dp"
                        android:layout_marginEnd="136dp"
                        android:layout_marginRight="136dp"
                        android:text="we may send notification about\nnewly added kahoots' challenges."
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/notifications"
                        android:layout_width="181dp"
                        android:layout_height="128dp"
                        android:layout_above="@+id/a1"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="13dp"
                        android:layout_marginLeft="13dp"
                        android:layout_marginTop="9dp"
                        android:layout_marginBottom="-60dp"
                        android:text="notifications"
                        android:textSize="30sp"
                        android:textStyle="bold" />

                    <Switch
                        android:id="@+id/switch2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/a11"
                        android:layout_alignStart="@+id/notifications"
                        android:layout_marginBottom="-111dp"
                        tools:ignore="RtlCompat" />

                </RelativeLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/freg2"
                android:layout_width="match_parent"
                android:layout_height="202dp"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="85dp"
                android:background="@color/colorPrimaryDark"
                android:orientation="vertical"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <RelativeLayout
                    android:id="@+id/alayout01"
                    android:layout_width="510dp"
                    android:layout_height="191dp"
                    android:layout_alignTop="@+id/freg2"
                    android:layout_margin="1dp"
                    android:layout_weight=".50"
                    android:background="@color/colorAccent"
                    android:gravity="center"
                    tools:ignore="Suspicious0dp">

                    <TextView
                        android:id="@+id/b1"
                        android:layout_width="202dp"
                        android:layout_height="25dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="139dp"
                        android:layout_marginLeft="139dp"
                        android:layout_marginTop="77dp"
                        android:layout_marginEnd="169dp"
                        android:layout_marginRight="169dp"
                        android:text="music"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/b11"
                        android:layout_width="258dp"
                        android:layout_height="57dp"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="116dp"
                        android:layout_marginLeft="116dp"
                        android:layout_marginTop="111dp"
                        android:layout_marginEnd="136dp"
                        android:layout_marginRight="136dp"
                        android:text="we may send notification about\nnewly added kahoots' challenges."
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/Game1"
                        android:layout_width="174dp"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/a1"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="31dp"
                        android:layout_marginLeft="31dp"
                        android:layout_marginTop="32dp"
                        android:layout_marginBottom="-60dp"
                        android:text="Game"
                        android:textSize="30sp"
                        android:textStyle="bold"
                        tools:ignore="NotSibling" />

                    <Switch
                        android:id="@+id/switch5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="72dp"
                        android:layout_marginLeft="72dp"
                        android:layout_marginBottom="50dp" />

                </RelativeLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/Notifications"
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:text="Notifications"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Social" />

            <TextView
                android:id="@+id/Game"
                android:layout_width="84dp"
                android:layout_height="22dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:text="Game"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.009"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Propile" />

            <TextView
                android:id="@+id/General"
                android:layout_width="80dp"
                android:layout_height="28dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:text="General"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Notifications" />

            <TextView
                android:id="@+id/Social"
                android:layout_width="87dp"
                android:layout_height="34dp"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:text="Social"
                android:textColor="#000000"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Game" />

        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="51dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/profileNamee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profileNamee"
        app:layout_constraintVertical_bias="0.072"
        app:srcCompat="@drawable/depositphotos_12_template"
        tools:ignore="UnknownId" />


</android.support.constraint.ConstraintLayout>